        <li><a class="shake" href= <%= main_register_path %> >Register</a></li>
      <% else %>
        <li><a href="">Logout</a></li>
        <li><a href="<%= admin_student_path %>">Survey form</a></li>
      <% end %>
      <li><a class="shake" href= <%= main_aboutus_path %> >Contact Us</a></li>
    </ul>
  </div>
</div>
<div class = "mainContent">
    <%= @student.name %>
</div>
</body>
</html>
